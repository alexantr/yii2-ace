{
  "name": "alexantr/yii2-ace",
  "description": "Ace editor widget for Yii 2",
  "keywords": ["ace", "code", "editor", "widget"],
  "type": "yii2-extension",
  "license": "MIT",
  "authors": [
    {
      "name": "Alex Yashkin",
      "email": "alex@yashkin.by"
    }
  ],
  "support": {
    "issues": "https://github.com/alexantr/yii2-ace/issues",
    "source": "https://github.com/alexantr/yii2-ace"
  },
  "require": {
    "yiisoft/yii2": "~2.0.5"
  },
  "autoload": {
    "psr-4": {
      "alexantr\\ace\\": "src"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
